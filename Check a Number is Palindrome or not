public class Main {
	public static void main(String[] args) {
		int n=1367631;
		System.out.print(checkPalin(n));
	}
public static String checkPalin(int num){
	
String s= String.valueOf(num);
int n=s.length();

if(s.length()==1){
	return "Yes";
}

for(int i=0; i<n/2; i++){
	if(s.charAt(i)==s.charAt(n-1-i)){
		return "Yes";
	}
}
return "No";
}


}
