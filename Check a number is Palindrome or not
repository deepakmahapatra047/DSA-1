public class Main {

	public static void main(String[] args) {		int n=1367631;

		

		if(checkPalin(n)){

		System.out.print("Yes");

		}

		else{

		System.out.print("No");

		}

	

	}

public static boolean checkPalin(int num){

	

String s= String.valueOf(num);

int n=s.length();

if(s.length()==1){

	return true;

}

for(int i=0; i<n/2; i++){

	

	 if(s.charAt(i)!=s.charAt(n-1-i)){

		return false;

	}

	

	

}

return true;

}

}
