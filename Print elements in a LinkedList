class Node{
	int data;
	Node next;
	Node(int data){
		this.data=data;
	}
}

public class Main {
	public static void main(String[] args) {
		Node n1= new Node(10);
		Node n2= new Node(20);
		Node n3= new Node(30);
		
		Node head= n1;
		n1.next=n2;
		n2.next=n3;
		n3.next=null;
		
		Node curr= head;
		
		while(curr !=null){
			
			System.out.print(curr.data+" ");
		curr=	curr.next;
		}
	}
}

